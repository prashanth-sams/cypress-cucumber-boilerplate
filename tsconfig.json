{
  "compileOnSave": false,
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "https://www.google.com/",
    "types": [
      "cypress"
    ],
    // "noEmit": true,
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "lib": ["es6", "dom", "es2017"],
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "cypress/integration/features/**/*.*"
  ]
}